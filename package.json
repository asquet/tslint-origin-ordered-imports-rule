{
  "name": "tslint-origin-ordered-imports-rule",
  "version": "1.1.1",
  "description": "Custom rule for tslint.",
  "main": "dist/originOrderedImportsRule.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcuzy/tslint-origin-ordered-imports-rule.git"
  },
  "scripts": {
    "watch": "nodemon -e js,ts,lint,fix,json --watch src --watch test --exec \"npm run compile && npm run test\"",
    "build": "tsc",
    "tslint-example": "tslint -c example/tslint.json example/*.ts",
    "test": "npm run test:rules",
    "prepublish": "npm run build",
    "compile": "npm-run-all -s compile:src compile:test",
    "compile:test": "tsc -p test",
    "compile:src": "tsc -p src",
    "test:rules": "node ./build/test/ruleTestRunner.js"
  },
  "keywords": [
    "tslint",
    "rule",
    "imports",
    "imports origin",
    "typescript"
  ],
  "author": "Oleg Krasavin <t5633@ya.ru>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.3.2",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.4",
    "chalk": "^2.4.1",
    "glob": "^7.1.2"
  },
  "dependencies": {
    "tsutils": "^2.27.1"
  }
}
